{
  "breadcrumbs.enabled": true,
  "editor.fontLigatures": true,
  "editor.codeLens": false,
  "editor.fontFamily": "Berkeley Mono Variable",
  "editor.fontSize": 14,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "Italic overrides",
        "scope": [
          "comment",
          "variable.parameter",
          "constant.language.symbol.hashkey.parameter.function.ruby",
          "variable.language.self.ruby",
          "entity.other.attribute-name.js.jsx",
          "variable.language.this.js",
          "text.html.basic entity.other.attribute-name.html"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "Italic exceptions",
        "scope": [
          "entity.name.type.instance.jsdoc",
          "storage.type.class.jsdoc"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "editor.minimap.enabled": false,
  "editor.insertSpaces": true,
  "extensions.ignoreRecommendations": true,
  "files.insertFinalNewline": true,
  "gitlens.keymap": "alternate",
  "gitlens.showWhatsNewAfterUpgrades": false,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true
  },
  "rust.jobs": 4,
  "rust.clippy_preference": "on",
  "rust-client.channel": "stable",
  "terminal.integrated.fontSize": 14,
  "vim.enableNeovim": true,
  "vim.useSystemClipboard": true,
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["\\"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.commentLine",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "j"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.formatSelection",
          "args": []
        }
      ]
    }
  ],
  "vim.disableExtension": false,
  "vim.leader": ",",
  "vim.hlsearch": true,
  "vim.neovimPath": "/usr/local/bin/nvim",
  "vim.showcmd": true,
  "vim.ignorecase": true,
  "vim.visualstar": true,
  "vim.incsearch": true,
  "vim.sneak": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "c"],
      "after": [],
      "commands": [
        {
          "command": ":nohl",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "j"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.formatDocument",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "w"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.trimTrailingWhitespace",
          "args": []
        },
        {
          "command": "workbench.action.files.save",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "f"],
      "after": [],
      "commands": [
        {
          "command": "workbench.files.action.showActiveFileInExplorer",
          "args": []
        }
      ]
    },
    {
      "before": ["\\"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.commentLine",
          "args": []
        }
      ]
    }
  ],
  "window.zoomLevel": 0,
  "workbench.fontAliasing": "antialiased",
  "workbench.sideBar.location": "right",
  "workbench.statusBar.feedback.visible": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "search.showLineNumbers": true,
  "workbench.editor.swipeToNavigate": true,
  "workbench.editor.highlightModifiedTabs": true,
  "ruby.codeCompletion": "rcodetools",
  "ruby.intellisense": "rubyLocate",
  "workbench.startupEditor": "newUntitledFile",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.rendererType": "dom",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.defaultLocation": "editor",
  "editor.lineHeight": 1.6,
  "rust-analyzer.checkOnSave.command": "clippy",
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "todo-tree.regex.regex": "(//|#|<!--|--|;|/\\*|^|^[ \\t]*(-|\\d+.))\\s*($TAGS)",
  "workbench.colorTheme": "Noctis Obscuro",
  "editor.stickyScroll.enabled": true,
  "redhat.telemetry.enabled": false,
  "rust-analyzer.imports.granularity.group": "module",
  "rust-analyzer.imports.prefix": "crate",
  "workbench.preferredDarkColorTheme": "Noctis Obscuro",
  "workbench.preferredLightColorTheme": "Noctis Lux",
  "window.autoDetectColorScheme": true,
  "todo-tree.highlights.enabled": false
}
